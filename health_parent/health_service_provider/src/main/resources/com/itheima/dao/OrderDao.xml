<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itheima.dao.OrderDao">

    <select id="findSetmealIdByMemberId" parameterType="java.lang.Integer" resultType="java.lang.Integer">
        SELECT setmeal_id FROM t_order where member_id = #{memberId}
    </select>

    <insert id="add" parameterType="com.itheima.pojo.Order">
        INSERT INTO t_order
        (member_id,orderDate,orderType,orderStatus,setmeal_id)
        VALUES
        (
        #{memberId},
        #{orderDate},
        #{orderType},
        #{orderStatus},
        #{setmealId}
        )


    </insert>
</mapper>